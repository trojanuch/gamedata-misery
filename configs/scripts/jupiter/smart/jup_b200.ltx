[smart_terrain]
squad_id = 9
max_population = 2
respawn_params = respawn@jup_b200   

[respawn@jup_b200]
spawn_freedom@advanced
spawn_freedom@veteran

spawn_freedom@advanced_hostile
spawn_freedom@veteran_hostile

; Freedom squads

[spawn_freedom@advanced]
spawn_squads = freedom_sim_squad_novice_jupiter
spawn_num = {+sim_duty_help_harder} 0, 1 ; {+jup_b207_task_end -jup_b218_gather_squad_complete} 0, {+jup_a12_task_done -jup_b218_gather_squad_complete} 0, {+jup_b46_task_done -jup_b218_gather_squad_complete} 0, {+jup_b8_heli_4_searched -jup_b218_gather_squad_complete} 0, {+jup_b209_task_done -jup_b218_gather_squad_complete} 0, {+jup_b218_gather_squad_complete} 0, 2

[spawn_freedom@veteran]
spawn_squads = freedom_sim_squad_veteran_jupiter, freedom_sci_simulation_squad_jupiter
spawn_num = {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b207_task_end} 1, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_a12_task_done} 1, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b46_task_done} 1, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b8_heli_4_searched} 1, {-sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b209_task_done} 1, {-sim_duty_help_harder +jup_b218_gather_squad_complete} 1, 0

; Freedom hostile squads

[spawn_freedom@advanced_hostile]
spawn_squads = freedom_sim_squad_novice_hostile_jupiter
spawn_num = {=squad_exist(freedom_sim_squad_novice_jupiter)} 0, {-sim_duty_help_harder} 0, 1 ; {+sim_duty_help_harder +jup_b207_task_end -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_a12_task_done -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_b46_task_done -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_b8_heli_4_searched -jup_b218_gather_squad_complete} 0, {+sim_duty_help_harder +jup_b209_task_done -jup_b218_gather_squad_complete} 0, {+jup_b218_gather_squad_complete} 0, 2

[spawn_freedom@veteran_hostile]
spawn_squads = freedom_sim_squad_veteran_hostile_jupiter, freedom_sci_simulation_squad_hostile_jupiter
spawn_num = {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b207_task_end} 1, {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder -jup_b218_gather_squad_complete +jup_a12_task_done} 1, {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b46_task_done} 1, {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b8_heli_4_searched} 1, {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder -jup_b218_gather_squad_complete +jup_b209_task_done} 1, {!squad_exist(freedom_sim_squad_advanced_veteran_jupiter) !squad_exist(freedom_sim_squad_veteran_jupiter) !squad_exist(freedom_sci_simulation_squad_jupiter) +sim_duty_help_harder +jup_b218_gather_squad_complete} 1, 0

