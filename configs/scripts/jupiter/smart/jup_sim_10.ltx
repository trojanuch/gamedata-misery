[smart_terrain]
squad_id = 30
max_population = 1
respawn_only_smart = true
respawn_params = respawn@jup_sim_10

[respawn@jup_sim_10]
spawn_boar
spawn_flesh

[spawn_boar]
spawn_squads = simulation_flesh, simulation_boar
spawn_num = {=actor_on_level(jupiter)} 1, 0

[spawn_flesh]
spawn_squads = simulation_flesh, simulation_boar
spawn_num = {=actor_on_level(jupiter)} 1, 0